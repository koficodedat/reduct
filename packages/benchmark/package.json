{
  "name": "@reduct/benchmark",
  "version": "0.1.0",
  "description": "Benchmarking infrastructure for Reduct",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./src/visualization/templates/**/*.ts",
    "./src/custom/**/*.ts",
    "./src/bin/**/*.ts"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "reduct-benchmark": "./dist/cli/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./runners": {
      "types": "./dist/runners/index.d.ts",
      "import": "./dist/runners/index.mjs",
      "require": "./dist/runners/index.js"
    },
    "./visualization": {
      "types": "./dist/visualization/index.d.ts",
      "import": "./dist/visualization/index.mjs",
      "require": "./dist/visualization/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "runners": [
        "./dist/runners/index.d.ts"
      ],
      "visualization": [
        "./dist/visualization/index.d.ts"
      ],
      "cli": [
        "./dist/cli/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/runners/index.ts src/visualization/index.ts src/cli/index.ts src/cli/bin.ts src/bin/run-list-size-benchmark.ts src/bin/run-list-implementation-benchmark.ts src/bin/analyze-list-benchmarks.ts src/bin/run-enhanced-list-benchmark.ts src/bin/run-comprehensive-benchmark.ts src/custom/size-variation-benchmark.ts src/custom/data-type-benchmark.ts src/custom/operation-pattern-benchmark.ts src/custom/immutability-benchmark.ts --format cjs,esm --dts",
    "test": "vitest run tests/",
    "test:watch": "vitest tests/",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "benchmark": "node dist/cli/bin.js",
    "benchmark:list-size": "node dist/bin/run-list-size-benchmark.js",
    "benchmark:list-implementation": "node dist/bin/run-list-implementation-benchmark.js",
    "benchmark:enhanced-list": "node dist/bin/run-enhanced-list-benchmark.js",
    "benchmark:comprehensive": "node dist/bin/run-comprehensive-benchmark.js",
    "analyze:list-benchmarks": "node dist/bin/analyze-list-benchmarks.js"
  },
  "dependencies": {
    "@reduct/algorithms": "^0.1.0",
    "@reduct/core": "^0.1.0",
    "@reduct/data-structures": "^0.1.0",
    "commander": "^13.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
